{
    "name": "hoai/laravel-translatable-main",
    "description": "Extend a Laravel package for multilingual models",
    "keywords": ["laravel", "translation", "language", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "nguyen van hoai",
            "email": "hoaigubkin@gmail.com",
            "homepage": "http://yanu.org"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.6.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Dimsav\\Translatable\\": "src/Translatable/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hoai\\Translatable\\TranslatableServiceProvider"
            ]
        }
    }
}
